<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Text?>


<AnchorPane prefHeight="398.0" prefWidth="326.0" xmlns="http://javafx.com/javafx/18" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.projectfx.LoginController">
   <children>
      <TextField id="emailTextField" fx:id="emailTextField" layoutX="68.0" layoutY="93.0" prefHeight="26.0" prefWidth="212.0" promptText="Enter your email" />
      <TextField id="passwordTextField" fx:id="passwordTextField" layoutX="68.0" layoutY="134.0" prefHeight="26.0" prefWidth="212.0" promptText="Enter your password" />
      <Button fx:id="loginButton" defaultButton="true" layoutX="68.0" layoutY="200.0" mnemonicParsing="false" onAction="#loginButtonClicked" prefHeight="26.0" prefWidth="148.0" text="Log In" />
      <Text fill="#4d98f4" layoutX="68.0" layoutY="181.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Forgot your password?" />
      <Text fill="#4d98f4" layoutX="68.0" layoutY="248.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Not registered yet?" />
      <Button id="registrationButton" fx:id="registrationButton" layoutX="68.0" layoutY="267.0" mnemonicParsing="false" onAction="#registrationButtonClicked" prefHeight="26.0" prefWidth="148.0" text="Register" />
   </children>
</AnchorPane>
